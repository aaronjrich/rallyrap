{
   "background": {
      "scripts": ["inject.js"],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "icon.png",
      "icons": [ "icon.png" ],
      "name": "Rally Raptorize"
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "jquery.raptorize.1.0.js", "rallyrap.js" ],
      "css": ["rallyrap.css"],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "options_ui": {
       "page": "options.html",
       "chrome_style": true
     },
   "description": "Instant Velociraptor on any page and for Rally",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Raptorize for Rally",
   "permissions": [ "webNavigation", "activeTab", "http://*/", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "web_accessible_resources": [ "raptor-sound.ogg", "raptor.png", "raptor-sound.mp3" ]
}
